local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

--- Window

local Window = OrionLib:MakeWindow({Name = "Harlen Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = "yes", IntroText = "Harlen Hub", IntroIcon = "https://harlenuuu.weebly.com/uploads/1/4/7/0/147031498/picsart-23-09-17-19-15-44-691_orig.png", Icon = "https://harlenuuu.weebly.com/uploads/1/4/7/0/147031498/picsart-23-09-17-19-15-44-691_orig.png"})

--- Tab

local Tab = Window:MakeTab({
	Name = "🌐┃Language",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--- meu Botao

Tab:AddButton({
	Name = "🇺🇸 • English",
	Callback = function()
      		OrionLib:Destroy() loadstring(game:HttpGet("https://raw.githubusercontent.com/Fe773Vdoa/LuckyBlockInglesUwhya6-twya6TJ76262727yY5/main/LuckyBlockInglesUwhya6-twya6TJ76262727yY5"))()
  	end    
})

Tab:AddButton({
	Name = "🇧🇷 • Português",
	Callback = function()
      		OrionLib:Destroy() loadstring(game:HttpGet("https://raw.githubusercontent.com/Fe773Vdoa/LuckyBlockPortuguesHejgF6RyIw65wG7-t74/main/LuckyBlockPortuguesHejgF6Rylw65wG7-74"))()
  	end    
})

OrionLib:Init()
