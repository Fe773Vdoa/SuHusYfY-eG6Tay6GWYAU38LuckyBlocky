local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

--- Window

local Window = OrionLib:MakeWindow({Name = "Harlen Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--- Tab

local Tab = Window:MakeTab({
	Name = "🌐┃Language",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--- meu Botao

